#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/interrupt-controller/irq.h>
#include <dt-bindings/input/input.h>

/ {
	aliases {
		rtc0 = &smc;
		rtc1 = &snvs_rtc;
		mmc0 = &usdhc3;
		mmc1 = &usdhc4;
	};

	chosen {
		stdout-path = &uart1;
	};

	reg_wifi: wifi_pwr {
		compatible = "regulator-fixed";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_reg_wifi>;
		regulator-name = "reg_wifi";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&gpio3 23 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-always-on;
	};

	reg_wifi_en: wifi_enable {
		compatible = "regulator-fixed";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_reg_wifi_en>;
		regulator-name = "reg_wifi_enable";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&gpio2 3 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		vin-supply = <&reg_wifi>;
		startup-delay-us = <50000>;
	};

	reg_wwan: wwan_enable {
		compatible = "regulator-fixed";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_reg_wwan>;
		regulator-name = "reg_wwan_enable";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&gpio6 3 GPIO_ACTIVE_HIGH>;
		enable-active-low;
		startup-delay-us = <300>;
		regulator-always-on;
	};

	reg_vref_adc: regulator-vref-adc {
		compatible = "regulator-fixed";
		regulator-name = "vref-adc";
		regulator-min-microvolt = <2500000>;
		regulator-max-microvolt = <2500000>;
	};

	reg_usb_otg: reg-usb-otg {
		compatible = "regulator-fixed";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_reg_usb_otg>;
		regulator-name = "reg_usb_otg";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		gpio = <&gpio3 22 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-always-on;
	};

	clk_32k_wifi: clk-32k-wifi {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <32768>;
	};

	gpio-keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpio_keys>;
		power {
			label = "Power Button";
			gpios = <&gpio3 29 GPIO_ACTIVE_LOW>;
			wakeup-source;
			linux,code = <KEY_POWER>;
		};
	};

	leds {
    	compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_leds>;
		status-green {
			label = "status-green";
			gpios = <&gpio1 20 GPIO_ACTIVE_HIGH>;
		};
		status-red {
			label = "status-red";
			gpios = <&gpio1 18 GPIO_ACTIVE_HIGH>;
		};
    };

	system-gpios {
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_system>;
		compatible = "drs,system-gpios";

		gpios = <&gpio4 23 GPIO_ACTIVE_HIGH>,
				<&gpio4 22 GPIO_ACTIVE_HIGH>,
				<&gpio4 21 GPIO_ACTIVE_HIGH>,
				<&gpio5 16 GPIO_ACTIVE_HIGH>,
				<&gpio5 17 GPIO_ACTIVE_HIGH>,
				<&gpio1 12 GPIO_ACTIVE_HIGH>,
				<&gpio1 14 GPIO_ACTIVE_HIGH>,
				<&gpio1 13 GPIO_ACTIVE_HIGH>,
				<&gpio4 26 GPIO_ACTIVE_HIGH>,
				<&gpio4 24 GPIO_ACTIVE_HIGH>,
				<&gpio4 25 GPIO_ACTIVE_HIGH>,
				<&gpio1 4 GPIO_ACTIVE_HIGH>,
				<&gpio6 2 GPIO_ACTIVE_HIGH>,
				<&gpio4 17 GPIO_ACTIVE_HIGH>,
				<&gpio4 27 GPIO_ACTIVE_LOW>,
				<&smc 0 GPIO_ACTIVE_HIGH>,
				<&smc 1 GPIO_ACTIVE_HIGH>,
				<&smc 2 GPIO_ACTIVE_HIGH>;
		gpio-line-names =	"sys_hw_setting0",
							"sys_hw_setting1",
							"sys_hw_setting2",
							"sys_ani1_current",
							"sys_ani2_current",
							"sys_gpi1",
							"sys_gpi2",
							"sys_gpi3",
							"sys_gps_antenna_power",
							"sys_gpo1",
							"sys_gpo2",
							"sys_mcu_reset",
							"sys_mcu_pwr_override",
							"sys_mcu_boot0",
							"sys_wwan_rst",
							"sys_gpo3",
							"sys_gpo4",
							"sys_ignition";
	};

	ani1-voltage {
		compatible = "voltage-divider";
		io-channels = <&adc 2>;
		output-ohms = <220>;
		full-ohms = <970>;
	};

	ani1-current {
		compatible = "current-sense-shunt";
		io-channels = <&adc 0>;
		shunt-resistor-micro-ohms = <100000000>;
	};

	ani2-voltage {
		compatible = "voltage-divider";
		io-channels = <&adc 3>;
		output-ohms = <220>;
		full-ohms = <970>;
	};

	ani2-current {
		compatible = "current-sense-shunt";
		io-channels = <&adc 1>;
		shunt-resistor-micro-ohms = <100000000>;
	};
};

&gpio1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_gpio1_hog>;

	gpio1-hog {
		gpio-hog;
		gpios = <2 GPIO_ACTIVE_HIGH>,
				<9 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "gpio1-hog";
	};
};

&gpio4 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_gpio4_hog>;

	gpio4-hog {
		gpio-hog;
		gpios = <10 GPIO_ACTIVE_HIGH>,
				<12 GPIO_ACTIVE_HIGH>,
				<18 GPIO_ACTIVE_HIGH>,
				<19 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "gpio4-hog";
	};
};

&gpio7 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_gpio7_hog>;

	gpio7-hog {
		gpio-hog;
		gpios = <13 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "gpio7-hog";
	};
};

&ecspi1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi1>;
	cs-gpios = <&gpio3 19 GPIO_ACTIVE_LOW>;
	status = "okay";

	gyro: st-gyro-spi@0 {
		reg = <0>;
		compatible = "st,lsm6dsm";
		spi-max-frequency = <10000000>;
		interrupt-parent = <&gpio5>;
		interrupts = <7 IRQ_TYPE_EDGE_RISING>, <8 IRQ_TYPE_EDGE_RISING>;
	};
};

&ecspi2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi2>;
	cs-gpios = <&gpio5 12 GPIO_ACTIVE_LOW>;
	status = "okay";

	adc: mcp3208@0 {
		#io-channel-cells = <1>;
		reg = <0>;
		compatible = "microchip,mcp3208";
		spi-max-frequency = <1000000>;
		vref-supply = <&reg_vref_adc>;
	};
};

&fec {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_enet>;
	phy-mode = "rgmii";
	status = "okay";
	phy-handle = <&arphy>;
	mdio {
		#address-cells = <1>;
		#size-cells = <0>;
		arphy: ethernet-phy@0 {
			compatible = "ethernet-phy-ieee802.3-c22";
			reg = <0x0>;
			max-speed = <100>;
			interrupt-parent = <&gpio1>;
			interrupts = <26 IRQ_TYPE_EDGE_FALLING>;
		};
	};
};

&wdog1 {
	status = "okay";
};

&wdog2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_wdog>;
	status = "okay";
};

&i2c2 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2>;
	status = "okay";

	onewire_1: ds2482@18 {
		reg = <0x18>;
		compatible = "dallas,ds2482";
	};

	pmic: pfuze100@8 {
		compatible = "fsl,pfuze100";
		reg = <0x08>;

		regulators {
			sw1a_reg: sw1ab {
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1875000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-ramp-delay = <6250>;
			};

			sw1c_reg: sw1c {
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1875000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-ramp-delay = <6250>;
			};

			sw2_reg: sw2 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
			};

			sw3a_reg: sw3a {
				regulator-min-microvolt = <400000>;
				regulator-max-microvolt = <1975000>;
				regulator-boot-on;
				regulator-always-on;
			};

			sw3b_reg: sw3b {
				regulator-min-microvolt = <400000>;
				regulator-max-microvolt = <1975000>;
				regulator-boot-on;
				regulator-always-on;
			};

			sw4_reg: sw4 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
			};

			swbst_reg: swbst {
				regulator-min-microvolt = <5000000>;
				regulator-max-microvolt = <5150000>;
				regulator-boot-on;
				regulator-always-on;
			};

			snvs_reg: vsnvs {
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <3000000>;
				regulator-boot-on;
				regulator-always-on;
			};

			vref_reg: vrefddr {
				regulator-boot-on;
				regulator-always-on;
			};

			vgen1_reg: vgen1 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <1550000>;
			};

			vgen2_reg: vgen2 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <1550000>;
			};

			vgen3_reg: vgen3 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
			};

			vgen4_reg: vgen4 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vgen5_reg: vgen5 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vgen6_reg: vgen6 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
			};
		};
	};
};

&i2c3 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c3>;
	status = "okay";

	smc: cargotec_smc@51 {
		compatible = "drs,ctgw";
		reg = <0x51>;
		#gpio-cells = <2>;
		gpio-controller;
		interrupt-parent = <&gpio5>;
		interrupts = <15 IRQ_TYPE_EDGE_FALLING>;
		has-rtc;
	};
};

&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1>;
	status = "okay";
};

&uart2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart2>;
	status = "okay";
};

&uart3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart3>;
	status = "okay";
};

&uart4 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart4>;
	status = "okay";
};

&uart5 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart5>;
	uart-has-rtscts;
	status = "okay";

	bluetooth {
        compatible = "ti,wl1837-st";
        enable-gpios = <&gpio2 4 GPIO_ACTIVE_HIGH>;
        clocks = <&clk_32k_wifi>;
        clock-names = "ext_clock";
	};
};

&reg_arm {
	vin-supply = <&sw1a_reg>;
};

&reg_pu {
	vin-supply = <&sw1c_reg>;
};

&reg_soc {
	vin-supply = <&sw1c_reg>;
};

&snvs_poweroff {
	status = "okay";
};

&snvs_pwrkey {
	status = "okay";
};

&usbh1 {
	status = "okay";
	disable-over-current;
};

&usbotg {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usbotg>;
	status = "okay";
	vbus-supply = <&reg_usb_otg>;
	disable-over-current;
	dr_mode = "host";
};

&usdhc4 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc4>;
	bus-width = <4>;
	non-removable;
	no-1-8-v;
	keep-power-in-suspend;
	cap-power-off-card;
	max-frequency = <25000000>;
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";
	vmmc-supply = <&reg_wifi_en>;

	wlcore: wlcore@2 {
		compatible = "ti,wl1837";
		reg = <2>;
		interrupt-parent = <&gpio2>;
		interrupts = <6 IRQ_TYPE_LEVEL_HIGH>;
		tcxo-clock-frequency = <26000000>;
	};
};

&usdhc3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc3>;
	bus-width = <8>;
	non-removable;
	no-1-8-v;
	keep-power-in-suspend;
	status = "okay";
};

&can1 {
	pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_flexcan1>;
    termination-gpios = <&gpio3 31 GPIO_ACTIVE_HIGH>;
	termination-ohms = <120>;
    status = "okay";
};

&can2 {
	pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_flexcan2>;
    termination-gpios = <&gpio1 6 GPIO_ACTIVE_HIGH>;
	termination-ohms = <120>;
    status = "okay";
};

&iomuxc {
	pinctrl_system: systemgrp {
		fsl,pins = <
			MX6QDL_PAD_DISP0_DAT0__GPIO4_IO21	0x80000000		/* HW Setting 0 */
			MX6QDL_PAD_DISP0_DAT1__GPIO4_IO22	0x80000000		/* HW Setting 1 */
			MX6QDL_PAD_DISP0_DAT2__GPIO4_IO23	0x80000000		/* HW Setting 2 */
			MX6QDL_PAD_DISP0_DAT22__GPIO5_IO16	0x00002060		/* EN_ANI1 */
			MX6QDL_PAD_DISP0_DAT23__GPIO5_IO17	0x00002060		/* EN_ANI2 */
			MX6QDL_PAD_DISP0_DAT3__GPIO4_IO24	0x00000060		/* GP_EN--GPO1  */
			MX6QDL_PAD_DISP0_DAT4__GPIO4_IO25	0x00000060		/* GP_EN--GPO2  */
			MX6QDL_PAD_SD2_DAT3__GPIO1_IO12     0x12000			/* GPI_1 */
			MX6QDL_PAD_SD2_DAT1__GPIO1_IO14     0x12000			/* GPI_2 */
			MX6QDL_PAD_SD2_DAT2__GPIO1_IO13     0x12000			/* GPI_3 */
			MX6QDL_PAD_DISP0_DAT6__GPIO4_IO27	0x0000b0b0 		/* GP_nPRST_WWAN */
			MX6QDL_PAD_DISP0_DAT5__GPIO4_IO26	0x00002060		/* GP_EN_PWR--ANT_GPS */
			MX6QDL_PAD_GPIO_4__GPIO1_IO04		0x80000000		/* MCU_RESET (active high)*/
			MX6QDL_PAD_DI0_PIN15__GPIO4_IO17	0x2070			/* MCU Boot select (active high) */
			MX6QDL_PAD_CSI0_DAT16__GPIO6_IO02	0x2070			/* GP-EN--BUCK_5V--CPU (active high) */
			/* unused pins */
			/*MX6QDL_PAD_DISP0_DAT7__GPIO4_IO28	0x0000b0b0*/	/* GP_nDISABLE_WWAN   */
			/*MX6QDL_PAD_DISP0_DAT8__GPIO4_IO29	0x0000b0b0*/	/* GP_nWAKE_L--WWAN   */
			/*MX6QDL_PAD_DISP0_DAT9__GPIO4_IO30	0x00002060*/	/* LED_nWWAN   */
			/*MX6QDL_PAD_ENET_CRS_DV__GPIO1_IO25  0x80000000*/	/* Phy reset (low) */
			/*MX6QDL_PAD_KEY_COL1__GPIO4_IO08 	0x1b060*/		/* TPM nRESET */
			/*MX6QDL_PAD_DISP0_DAT10__GPIO4_IO31 	0x1b060*/	/* GP_Status--CPU */
			/*MX6QDL_PAD_NANDF_RB0__GPIO6_IO10    0x0001b000*/	/* PMIC_INT_B */
			/*MX6QDL_PAD_NANDF_D5__GPIO2_IO05     0x80000000*/	/* WL_IRQ (NA) */
			/*MX6QDL_PAD_DISP0_DAT11__GPIO5_IO05 	0x1b040*/	/* RTC */
            /*MX6QDL_PAD_DISP0_DAT12__GPIO5_IO06 	0x1b040*/	/* RTC */
			/*MX6QDL_PAD_SD1_DAT0__GPIO1_IO16		0x80000000*//* CAN1_ERR_N */
			/*MX6QDL_PAD_SD1_DAT1__GPIO1_IO17		0x80000000*//* CAN2_ERR_N */
		>;
	};

	pinctrl_ecspi1: ecspi1grp {
		fsl,pins = <
			MX6QDL_PAD_EIM_D17__ECSPI1_MISO		0x100b1
			MX6QDL_PAD_EIM_D18__ECSPI1_MOSI		0x100b1
			MX6QDL_PAD_EIM_D16__ECSPI1_SCLK		0x100b1
			MX6QDL_PAD_EIM_D19__GPIO3_IO19		0x100b1			/* Gyro CS */
			MX6QDL_PAD_DISP0_DAT13__GPIO5_IO07 	0x1b040			/* Gyro IRQ */
			MX6QDL_PAD_DISP0_DAT14__GPIO5_IO08 	0x1b040			/* Gyro IRQ */
		>;
	};

	pinctrl_ecspi2: ecspi2grp {
		fsl,pins = <
			MX6QDL_PAD_DISP0_DAT17__ECSPI2_MISO  0x100b1
			MX6QDL_PAD_DISP0_DAT16__ECSPI2_MOSI 0x100b1
			MX6QDL_PAD_DISP0_DAT19__ECSPI2_SCLK 0x100b1
			MX6QDL_PAD_DISP0_DAT18__GPIO5_IO12  0x100b1		/* ADC CS */
			/*MX6QDL_PAD_DISP0_DAT15__GPIO5_IO09  0x100b1*/ /* TPM CS */
		>;
	};

	pinctrl_flexcan1: flexcan1grp {
		fsl,pins = <
			MX6QDL_PAD_GPIO_7__FLEXCAN1_TX 	0x1b0b1
			MX6QDL_PAD_GPIO_8__FLEXCAN1_RX	0x1b0b1
			MX6QDL_PAD_EIM_D31__GPIO3_IO31  0x00000068			/* GP_CAN1_RES_EN */
		>;
	};

	pinctrl_flexcan2: flexcan2grp {
		fsl,pins = <
			MX6QDL_PAD_KEY_COL4__FLEXCAN2_TX 	0x1b0b1
			MX6QDL_PAD_KEY_ROW4__FLEXCAN2_RX	0x1b0b1
			MX6QDL_PAD_GPIO_6__GPIO1_IO06	0x00000068			/* CAN2_RES */
		>;
	};

	pinctrl_uart1: uart1grp {
		fsl,pins = <
			MX6QDL_PAD_CSI0_DAT10__UART1_TX_DATA    0x1b0b1
			MX6QDL_PAD_CSI0_DAT11__UART1_RX_DATA    0x1b0b1
		>;
	};

	pinctrl_uart2: uart2grp {
		fsl,pins = <
			MX6QDL_PAD_EIM_D26__UART2_TX_DATA    0x1b0b1
			MX6QDL_PAD_EIM_D27__UART2_RX_DATA    0x1b0b1
		>;
	};

	pinctrl_uart3: uart3grp {
		fsl,pins = <
			MX6QDL_PAD_EIM_D24__UART3_TX_DATA   0x1b0b1
			MX6QDL_PAD_EIM_D25__UART3_RX_DATA   0x1b0b1
		>;
	};

	pinctrl_uart4: uart4grp {
		fsl,pins = <
			MX6QDL_PAD_KEY_COL0__UART4_TX_DATA  0x1b0b1
			MX6QDL_PAD_KEY_ROW0__UART4_RX_DATA  0x1b0b1
		>;
	};

	pinctrl_uart5: uart5grp {
		fsl,pins = <
			MX6QDL_PAD_CSI0_DAT14__UART5_TX_DATA 0x1b0b1
			MX6QDL_PAD_CSI0_DAT15__UART5_RX_DATA 0x1b0b1
			MX6QDL_PAD_CSI0_DAT18__UART5_RTS_B   0x1b0b1
			MX6QDL_PAD_CSI0_DAT19__UART5_CTS_B   0x1b0b1
			MX6QDL_PAD_NANDF_D4__GPIO2_IO04     0x00002070      /* BT_REG_ON (Bluetooth power) */
		>;
	};

	pinctrl_usbotg: usbotggrp {
		fsl,pins = <
			MX6QDL_PAD_ENET_RX_ER__USB_OTG_ID   0x17059
			/*MX6QDL_PAD_KEY_ROW1__GPIO4_IO09		0x17059*/			/* USB_OTG_OC */
		>;
	};

	pinctrl_usdhc3: usdhc3grp {
		fsl,pins = <
			MX6QDL_PAD_SD3_CMD__SD3_CMD     	0x17059
			MX6QDL_PAD_SD3_CLK__SD3_CLK     	0x10059
			MX6QDL_PAD_SD3_DAT0__SD3_DATA0      0x17059
			MX6QDL_PAD_SD3_DAT1__SD3_DATA1      0x17059
			MX6QDL_PAD_SD3_DAT2__SD3_DATA2      0x17059

&i2c3 {
	clock-frequency = <400000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c3>;
	status = "okay";

	pca6416: gpio@20 {
		compatible = "ti,tca6416";
		reg = <0x20>;
		gpio-controller;
		#gpio-cells = <2>;
	};

	vmcu: vmcu@51 {
		compatible = "drs,vmcu";
		reg = <0x51>;
		#io-channel-cells = <1>;
		#gpio-cells = <2>;
		gpio-controller;
		interrupt-parent = <&gpio3>;
		interrupts = <20 IRQ_TYPE_EDGE_FALLING>, <22 IRQ_TYPE_EDGE_FALLING>;
		interrupt-names = "irq", "shutdown";
	};
};

&ecspi1 {
	cs-gpios = <&gpio5 9 GPIO_ACTIVE_LOW>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi1>;
	status = "okay";

	flash0: mx25l3233fm2i-08g@0 {
		reg = <0>;
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		/* Unreliable communication at 50MHz, quite possibly due to soldered in wires, cap at 10MHz */
		spi-max-frequency = <10000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			spl@0 {
				label = "spl";
				reg = <0x000000 0x040000>;
			};

			platform@40000 {
				label = "platform";
				reg = <0x040000 0x010000>;
				read-only;
			};

			u-boot@50000 {
				label = "u-boot";
				reg = <0x050000 0x1A0000>;
			};

			u-boot-second@1F0000 {
				label = "u-boot-second";
				reg = <0x1F0000 0x1A0000>;
			};

			system_a@390000 {
				label = "system_a";
				reg = <0x390000 0x010000>;
			};

			system_b@3A0000 {
				label = "system_b";
				reg = <0x3A0000 0x010000>;
			};

			user_a@3B0000 {
				label = "user_a";
				reg = <0x3B0000 0x010000>;
			};

			user_b@3C0000 {
				label = "user_b";
				reg = <0x3C0000 0x010000>;
			};
		};
	};
};

